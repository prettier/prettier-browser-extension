{
  "name": "Prettier",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Prettier all the things.",
  "homepage_url": "https://prettier.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "dist/options.html",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://stackoverflow.com/*", "https://github.com/*"],
      "js": ["dist/content.js"],
      "run_at": "document_end"
    }
  ]
}
