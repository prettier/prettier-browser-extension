{
  "name": "Prettier",
  "manifest_version": 2,
  "description": "Prettier all the things.",
  "homepage_url": "https://prettier.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://stackoverflow.com/*",
        "https://askubuntu.com/*",
        "https://mathoverflow.com/*",
        "https://serverfault.com/*",
        "https://stackapps.com/*",
        "https://superuser.com/*",
        "https://*.stackexchange.com/*",
        "https://github.com/*",
        "https://leetcode.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
